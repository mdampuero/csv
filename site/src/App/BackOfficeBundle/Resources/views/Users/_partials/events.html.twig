<div class="p-t-20 p-b-30">
    <h4 class="card-title">{{ 'ACCESS_TO_EVENTS' | trans }}</h4>
    <hr>
    <div class="row" id="resultProjects">
        {% block events %}
            {% spaceless %}
                {% for child in form.events %}
                    <div class="col-md-6 col-lg-4 {{ child.vars.value }}">
                        {{ form_widget(child) }}
                        {{ form_label(child) }}
                    </div>
                {% endfor %}
            {% endspaceless %}
        {% endblock events %}
    </div>
</div>
